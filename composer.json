{
    "name": "vgrdominik/elasticsearch-bundle",
    "type": "symfony-bundle",
    "description": "Elasticsearch management for Symfony2 applications. Manager, entities and repositories.",
    "keywords": ["elasticsearch", "entity", "manager", "repository"],
    "homepage": "https://github.com/vgrdominik/VGRElasticsearchBundle/wiki",
    "license": "MIT",
    "authors": [
        {
            "name":     "Valentí Gàmez Rojas",
            "homepage": "http://vgrdominik.es"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/vgrdominik/VGRElasticsearchBundle/contributors"
        }
    ],
    "require": {
        "php":                              ">=5.3.3",
        "symfony/framework-bundle":         ">=2.3"
    },
    "autoload": {
        "psr-4": { "VGR\\ElasticsearchBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev"
}